   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "\n",
    "r = 2 # m.\n",
    "a = 9.8 # accelerazione centripeta, m/s^2.\n",
    "\n",
    "# Velocità scalare.\n",
    "v = np.sqrt(a*r)\n",
    "print(v, 'm/s')\n",
    "\n",
    "# Velocità angolare.\n",
    "w = np.sqrt(a/r)\n",
    "\n",
    "# Frequenza di rotazione.\n",
    "f = w/(2*np.pi)\n",
    "print(f, 'giri/s')"
   ]
  }
